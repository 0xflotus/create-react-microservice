    "react-redux": "^5.0.5",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-lumbergh": "1.0.5",
    "redux-saga": "^0.15.4",
    "reselect": "^3.0.1",
