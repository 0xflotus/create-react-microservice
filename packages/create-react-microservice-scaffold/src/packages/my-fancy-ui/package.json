    "@company-scope/my-fancy-ui-logger": "0.0.0-development",
    "classcat": "^1.1.0",
    "empty": "^0.10.1",
    "hypernova": "^2.1.3",
    "hypernova-react": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
